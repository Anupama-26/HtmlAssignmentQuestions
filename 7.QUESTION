<!-- Q.7 Create a simple Music player using html only -->

<!DOCTYPE html>
<html>
<head>
  <title>Simple Music Player</title>
</head>
<body>
  <h1>Simple Music Player</h1>
  <audio controls>
    <source src="song.mp3" type="audio/mpeg">
    Your browser does not support the audio element.
  </audio>
</body>
</html>
